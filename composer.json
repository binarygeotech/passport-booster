{
    "name": "binarygeotech/passport-booster",
    "description": "Adds multiple authentication functionality to Laravel Passport",
    "license": "MIT",
    "require": {
		"php": "^7.2",
        "laravel/passport": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "BGS\\PassportBooster\\": "src/"
        }
    },
    "authors": [{
        "name": "George Okojie Davis",
        "email": "binarygeotech@users.noreply.github.com"
    }],
    "extra": {
        "laravel": {
            "providers": [
                "BGS\\PassportBooster\\PassportBoosterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}